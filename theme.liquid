    e.preventDefault();
	jQuery(".whole_bg").css("width", "100%");
  	jQuery(".cart_wrapper").addClass("translate_me");
  	jQuery("body").css("overflow", "hidden");
  	
});
$('.whole_bg').click(function () {
	jQuery(".cart_wrapper").removeClass("translate_me");
  	jQuery(".whole_bg").css("width", "0");
  	jQuery("body").css("overflow", "scroll");
});
} else {
  $(".cart-contents").click(function (e) {

    e.preventDefault();
    if($(".side_cart").hasClass("jaggu_ki_bhains")){
      jQuery(".side_cart").removeClass("jaggu_ki_bhains");
      jQuery("body").css("overflow", "scroll");
    } else {
      jQuery(".side_cart").addClass("jaggu_ki_bhains");
      jQuery("body").css("overflow", "hidden");
    }
  	
});
}

});


</script>

